[
  {"name":"id", "class":"id", "start":1},
  { 
    "name":"Name", "class":"lookup", "resource":"active_companynames.csv"
  },
  {"name":"accountnumber", "class":"id", "start":1},
  {"name":"AcctType", "class":"string", "dist":{"Customer - Direct":0.5, "Customer - Channel":0.5, "Other":0.02}},
  {"name":"BillingStreet", "class":"address"},
  {"name":"BillingCity", "class": "zip", "fields": "city" },
  {"name":"BillingState", "class": "state"},
  {
       "name": "BillingPostalCode",
       "class": "zip",
       "seed": 12,
       "fields": "zip",
       "onlyContinental": "true"
  },
  {"name": "Phone", "class": "join", "separator": "-", "value": {
          "class":"sequence",
          "length":3,
          "array":[
              { "class": "int", "min": 100, "max": 999},
              { "class": "int", "min": 100, "max": 999},
              { "class": "int", "min": 1000, "max": 9999}
          ]
  }},
  {"name": "Fax", "class": "join", "separator": "-", "value": {
          "class":"sequence",
          "length":3,
          "array":[
              { "class": "int", "min": 100, "max": 999},
              { "class": "int", "min": 100, "max": 999},
              { "class": "int", "min": 1000, "max": 9999}
          ]
  }},
  {"name": "Email", "class": "join", "separator": "", "value": {
          "class":"sequence",
          "length":3,
          "array":[
              { "class": "name", "type": "first"},
              { "class": "name", "type": "last"},
              { "class": "string", "dist":{"@example.com":1.0}}
          ]
  }},
  {"name":"Status", "class":"string", "dist":{"Active":1.0, "InActive":0.0}},
  {"name":"DateStarted", "class": "date", "format":"yyyy-MM-dd HH:mm:ss","start":"2000-01-31 09:00:00", "end":"2019-08-31 23:00:00"},
  {"name":"DateEnded", "class": "string", "dist":{"2999-12-31 00:00:00":1.0}}
]
